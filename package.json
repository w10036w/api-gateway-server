{
  "name": "api-gateway",
  "version": "0.1.0",
  "description": "API Gateway Server built with koa, graphql",
  "main": "index.js",
  "scripts": {
    "benchmark": "cross-env node benchmark",
    "build": "cross-env EXCLUDE_MODULES=true npm run webpack:build",
    "build:bundle": "cross-env EXCLUDE_MODULES=false npm run webpack:build",
    "deploy": "cross-env NODE_ENV=production serverless --stage=dev deploy",
    "dev": "npm run server",
    "dev:all": "npm-run-all --parallel server playground",
    "handler": "cross-env NODE_ENV=development serverless offline start --dontPrintOutput",
    "analyze": "cross-env EXCLUDE_MODULES=false npm run webpack:analyze",
    "playground": "graphql playground",
    "server": "cross-env NODE_ENV=development nodemon src/server -w .env.js -w src --exec babel-node --color",
    "setenv": "node scripts/setenv.js",
    "start": "cross-env NODE_ENV=production node dist/main",
    "start:pandora": "./node_modules/.bin/pandora start --name api-gateway --env=\"NODE_ENV=production\" --node-args=\"--expose-gc\"",
    "start:monitor": "./node_modules/.bin/pandora start --name api-gateway-dashboard `pandora-dashboard-dir`",
    "start:handler": "cross-env NODE_ENV=production serverless --stage=dev offline start --dontPrintOutput",
    "webpack:build": "cross-env NODE_ENV=production webpack --config scripts/webpack/webpack.server.js --progress --hide-modules",
    "webpack:analyze": "cross-env NODE_ENV=production webpack --config scripts/webpack/webpack.analyze.js --progress --hide-modules",
    "webpack:monitor": "cross-env NODE_ENV=production webpack --config scripts/webpack/webpack.monitor.js --progress --hide-modules"
  },
  "devDependencies": {
    "0x": "^4.8.2",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
